{
  "name": "template-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:dev": "run-p start:ng gql:codegen:watch",
    "start:pwa": "pushd dist/OAShoppingList; python -m SimpleHTTPServer; popd",
    "postinstall": "ng build  --aot --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gql:update-schema": "apollo client:download-schema schema.graphql  --config=apollo.config.js",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "gql:codegen:watch": "graphql-codegen --watch",
    "bundle-report": "webpack-bundle-analyzer dist/OAShoppingList/stats-es2015.json"
  },
